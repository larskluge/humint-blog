<!DOCTYPE html>
<html lang="en">
  <head>
    {{! Document Settings }}
    <meta http-equiv="Content-Type" charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    {{! Page Meta }}
    <title>{{meta_title}}</title>
    <meta name="description" content="{{meta_description}}">
    <meta name="author" content="Aestik">
    <meta name="viewport" content="width=device-width, initial-scale=1.0,maximum-scale=1">

    {{! Styles }}
    <link href="http://fonts.googleapis.com/css?family=Maven+Pro:400,500,700,900" rel="stylesheet">
    <link href="http://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700&amp;subset=latin,cyrillic" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Noto+Sans:400,700,400italic,700italic&amp;subset=cyrillic,latin" rel="stylesheet" type="text/css">
    <link href="{{@blog.url}}/assets/css/bootstrap.css" rel="stylesheet">
    <link href="{{@blog.url}}/assets/css/fonticon.css" rel="stylesheet">
    <link href="{{@blog.url}}/assets/css/nprogress.css" rel="stylesheet">
    <link href="{{@blog.url}}/assets/css/style.css" rel="stylesheet">
    <!-- <HTML5>shim, for IE6-8 support of HTML5 elements </HTML5> -->
    <!--[if lt IE 9]>
      <script src="{{@blog.url}}/assets/js/vendor/html5shiv.js"></script>
    <![endif]-->
  </head>
  <body class="suave {{body_class}}">
    {{!-- Header & Navigations --}}
    <header role="banner">
      {{!-- Logo & Nav --}}
      <nav role="navigation" class="navbar navbar-default main-navigation">
        <div class="container">
          <div class="navbar-header">
            {{!-- Logo and Title --}}
            <a href="{{@blog.url}}" class="navbar-brand">{{#if @blog.logo}}<img src="{{@blog.url}}{{@blog.logo}}" alt="">{{/if}}{{@blog.title}}</a>
            {{!-- Blog description --}}
            {{#if @blog.description}}
              <span class="brand-tagline pull-left">{{@blog.description}}</span>
            {{/if}}
          </div>
          <div class="navbar-collapse">
            <ul class="nav navbar-nav pull-right">
              {{!-- [HOME PAGES SWITCHER MENU] }}
              <li class="dropdown">
                <a href="#" data-toggle="dropdown" role="button" class="dropdown-toggle">Home Pages<i class="aecon-angle-down"></i></a>
                <ul role="menu" class="dropdown-menu">
                  <li role="presentation">
                    <a href="http://demo.aestik.com/suave-ghost/def/" role="menuitem">Home Page 1</a>
                  </li>
                  <li role="presentation">
                    <a href="http://demo.aestik.com/suave-ghost/alt/" role="menuitem">Home Page 2</a>
                  </li>
                </ul>
              </li>{{!-- end [HOME PAGES SWITCHER MENU] --}}
            </ul>
          </div>
        </div>
      </nav>
    </header>

    <main id="wrapper" role="main">
      {{!-- Headline --}}
      <div class="headline">
        <div class="image covered">
          <div class="container full">
            {{!-- Output the first post on pagination --}}
            {{#each posts}}
              {{#if @first}}
                <article class="vertically centered hero unit">
                  <header>
                    {{!-- post date --}}
                    <time class="post date" datetime="{{date format="YYYY-MM-DD"}}">{{date format='DD MMM YYYY'}}</time>
                    {{!-- post title --}}
                    <h1><a href="{{url}}">{{title}}</a></h1>
                  </header>
                </article>
                {{!-- post read more --}}
                <a href="{{url}}" class="btn btn-primary btn-lg btn-cta">Read More</a>
              {{/if}}
            {{/each}}
          </div>
        </div>
      </div>{{!-- /Headline --}}
      {{!-- Output the rest of the posts --}}
      {{#each posts}}
        {{#unless @first}}
          <article class="{{post_class}} excerpt container">
            <header>
              <span class="badge">featured</span>
              {{!-- post date --}}
              <time class="post date" datetime="{{date format="YYYY-MM-DD"}}">{{date format='DD MMM YYYY'}}</time>
              {{!-- post title --}}
              <h2><a href="{{url}}">{{title}}</a></h2>
            </header>
          </article>
        {{/unless}}
      {{/each}}
      {{!-- Output pagination --}}
      <div class="pagination holder">
        {{pagination}}
      </div>
    </main>

    <!-- FOOTER-->
    <footer role="contentinfo">
      All content on Humint Labs is <a href="http://creativecommons.org/licenses/by-nc-sa/4.0/" target="_blank">Creative Commons licensed</a>, for non-commercial sharing  with attribution
      <br>
      <a href="http://www.humint.is" target="_blank">Humint</a> 2014
    </footer>

    {{!-- google analytics --}}
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-46858420-2', 'humint.is');
      ga('send', 'pageview');
    </script>
    {{!-- jquery --}}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
    {{!-- jquery ui --}}
    <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
    {{!-- jquery cookie --}}
    <script src="{{@blog.url}}/assets/js/vendor/jquery.cookie.js"></script>
    {{!-- jquery FitVids --}}
    <script src="{{@blog.url}}/assets/js/vendor/jquery.fitvids.js"></script>
    {{!-- NProgress --}}
    <script src="{{@blog.url}}/assets/js/vendor/nprogress.js"></script>
    {{!-- bootstrap --}}
    <script src="{{@blog.url}}/assets/js/vendor/bootstrap.min.js"></script>
    {{!-- Respond js Polyfill --}}
    <script src="{{@blog.url}}/assets/js/vendor/respond.min.js"></script>
    {{!-- script --}}
    <script src="{{@blog.url}}/assets/js/script.js"></script>
  </body>
</html>

